<h1>Actors</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
  </tr>

  <% @actors.each do |actor| %>
    <tr>
      <td><%= actor.first_name %></td>
      <td><%= actor.last_name %></td>
      <td><%= link_to "Go to", actor_path(actor) %>
    </tr>
  <% end %>
</table>

<br>
<%= link_to "Add Actor", new_actor_path %>
